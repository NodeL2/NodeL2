{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "id": {
                "type": "integer"
            },
            "template": {
                "type": "object",
                "properties": {
                    "description": {
                        "type": "string"
                    },
                    "price": {
                        "type": "integer"
                    }
                },
                "required": [
                    "description",
                    "price"
                ]
            },
            "spawns": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "locX": {
                            "type": "integer"
                        },
                        "locY": {
                            "type": "integer"
                        },
                        "locZ": {
                            "type": "integer"
                        }
                    },
                    "required": [
                        "locX",
                        "locY",
                        "locZ"
                    ]
                }
            }
        },
        "required": [
            "id",
            "template",
            "spawns"
        ]
    }
}
