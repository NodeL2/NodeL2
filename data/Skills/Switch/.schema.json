{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "selfId": {
                "type": "integer"
            },
            "template": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "passive": {
                        "type": "boolean"
                    }
                },
                "required": [
                    "name",
                    "passive"
                ]
            },
            "levels": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "level": {
                            "type": "integer"
                        },
                        "power": {
                            "type": "integer"
                        },
                        "mp": {
                            "type": "integer"
                        },
                        "hp": {
                            "type": "integer"
                        },
                        "itemId": {
                            "type": "integer"
                        },
                        "itemCount": {
                            "type": "integer"
                        }
                    },
                    "required": [
                        "level",
                        "power",
                        "mp",
                        "hp",
                        "itemId",
                        "itemCount"
                    ]
                }
            }
        },
        "required": [
            "selfId",
            "template",
            "levels"
        ]
    }
}
