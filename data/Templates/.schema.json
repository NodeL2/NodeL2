{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "classId": {
                "type": "integer"
            },
            "template": {
                "type": "object",
                "properties": {
                    "class": {
                        "type": "string"
                    },
                    "race": {
                        "type": "integer"
                    },
                    "crafter": {
                        "type": "boolean"
                    }
                },
                "required": [
                    "class",
                    "race",
                    "crafter"
                ]
            },
            "base": {
                "type": "object",
                "properties": {
                    "str": {
                        "type": "integer"
                    },
                    "dex": {
                        "type": "integer"
                    },
                    "con": {
                        "type": "integer"
                    },
                    "int": {
                        "type": "integer"
                    },
                    "wit": {
                        "type": "integer"
                    },
                    "men": {
                        "type": "integer"
                    }
                },
                "required": [
                    "str",
                    "dex",
                    "con",
                    "int",
                    "wit",
                    "men"
                ]
            },
            "stats": {
                "type": "object",
                "properties": {
                    "pAtk": {
                        "type": "integer"
                    },
                    "mAtk": {
                        "type": "integer"
                    },
                    "pDef": {
                        "type": "integer"
                    },
                    "mDef": {
                        "type": "integer"
                    },
                    "crit": {
                        "type": "integer"
                    },
                    "atkSpd": {
                        "type": "integer"
                    },
                    "castSpd": {
                        "type": "integer"
                    }
                },
                "required": [
                    "pAtk",
                    "mAtk",
                    "pDef",
                    "mDef",
                    "crit",
                    "atkSpd",
                    "castSpd"
                ]
            },
            "vitals": {
                "type": "object",
                "properties": {
                    "maxHp": {
                        "type": "integer"
                    },
                    "maxMp": {
                        "type": "integer"
                    },
                    "maxLoad": {
                        "type": "integer"
                    }
                },
                "required": [
                    "maxHp",
                    "maxMp",
                    "maxLoad"
                ]
            },
            "speed": {
                "type": "object",
                "properties": {
                    "walk": {
                        "type": "integer"
                    },
                    "run": {
                        "type": "integer"
                    },
                    "swim": {
                        "type": "integer"
                    }
                },
                "required": [
                    "walk",
                    "run",
                    "swim"
                ]
            },
            "collision": {
                "type": "object",
                "properties": {
                    "radius": {
                        "type": "number"
                    },
                    "size": {
                        "type": "number"
                    }
                },
                "required": [
                    "radius",
                    "size"
                ]
            }
        },
        "required": [
            "classId",
            "template",
            "base",
            "stats",
            "vitals",
            "speed",
            "collision"
        ]
    }
}
